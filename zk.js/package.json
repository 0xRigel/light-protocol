    "axios": "^1.5.0",
    "case-anything": "^2.1.13",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "decimal.js": "^10.4.3",
    "ethereum-cryptography": "^2.0.0",
    "ffjavascript": "^0.2.62",
    "fs": "^0.0.1-security",
    "global": "^4.4.0",
    "snarkjs": "^0.7.2",
    "tweetnacl": "^1.0.3"
