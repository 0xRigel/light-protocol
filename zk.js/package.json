{
  "name": "@lightprotocol/zk.js",
  "version": "0.3.2-alpha.16",
  "description": "",
  "type": "module",
  "main": "./dist/umd/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/main/index.d.ts",
      "import": "./dist/es/index.js",
      "default": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/types/main/index.d.ts",
  "files": [
    "dist",
    "build-circuits",
    "bin",
    "circuit-lib"
  ],
  "scripts": {
    "start-test-validator": "./../cli/test_bin/run test-validator -b",
    "test": "pnpm test-zk.js && pnpm test-system-programs",
    "test-zk.js": "vitest run --test-timeout=1000000",
    "test-utxo": "vitest run tests/utxo.test.ts",
    "test-bn254": "vitest run tests/bn254.test.ts",
    "test-program-utxo": "vitest run tests/program-utxo.test.ts",
    "test-verifiers": "vitest run tests/verifiers.test.ts --test-timeout=1000000",
    "test-circuits": "vitest run tests/circuits.test.ts",
    "test-prover": "vitest run tests/prover.test.ts --test-timeout=1000000",
    "test-account": "vitest run tests/account.test.ts",
    "test-rpc": "vitest run tests/rpc.test.ts",
    "test-createOutUtxos": "vitest run tests/createOutUtxos.test.ts",
    "test-provider": "vitest run tests/provider.test.ts",
    "test-selectInUtxos": "vitest run tests/selectInUtxos.test.ts",
    "test-balance": "vitest run tests/balance.test.ts",
    "test-convertDecimals": "vitest run tests/convertDecimals.test.ts",
    "test-system-programs": "pnpm run test-sp-functional && pnpm run test-sp-user && pnpm run test-sp-merkle-tree && pnpm test-sp-user-merge && pnpm run test-sp-verifiers && pnpm run test-sp-user-registry",
    "test-sp-functional": "pnpm run start-test-validator && vitest run tests/system-programs/functional_tests.ts",
    "test-sp-user": "pnpm run start-test-validator && vitest run tests/system-programs/user.tests.ts",
    "test-sp-user-random": "pnpm run start-test-validator && vitest run tests/system-programs/user-random.tests.ts",
    "test-sp-user-merge": "pnpm run start-test-validator && vitest run tests/system-programs/user-merge.tests.ts",
    "test-sp-verifiers": "pnpm run start-test-validator && vitest run tests/system-programs/verifier_tests.ts",
    "test-sp-merkle-tree": "./../cli/test_bin/run test-validator -b -s && vitest run tests/system-programs/merkle_tree_tests.ts",
    "test-sp-user-registry": "pnpm run start-test-validator && vitest run tests/system-programs/user-registry.tests.ts --test-timeout=2000000",
    "build": "rimraf dist && pnpm build:bundle",
    "build:bundle": "rollup -c --bundleConfigAsCjs",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"tests/**/*.{ts,js}\" -w",
    "format:check": "prettier \"src/**/*{ts,js}\" --check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "format-staged": {
    "**/*.{js,jsx}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "sdk",
    "light",
    "lightprotocol"
  ],
  "author": "ananas-block",
  "license": "GPL-3.0",
  "dependencies": {
    "@coral-xyz/anchor": "0.28.0",
    "@coral-xyz/borsh": "0.28.0",
    "@lightprotocol/account.rs": "workspace:*",
    "@lightprotocol/circuit-lib.js": "workspace:*",
    "@lightprotocol/prover.js": "workspace:*",
    "@noble/hashes": "^1.3.2",
    "@solana/spl-account-compression": "^0.1.8",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.5.0",
    "case-anything": "^2.1.13",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "decimal.js": "^10.4.3",
    "ethereum-cryptography": "^2.0.0",
    "ffjavascript": "^0.2.62",
    "fs": "^0.0.1-security",
    "global": "^4.4.0",
    "snarkjs": "^0.7.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@lightprotocol/tsconfig": "workspace:*",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.9",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.10.2",
    "@types/node-fetch": "^2.6.2",
    "@types/seedrandom": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "circom_tester": "^0.0.19",
    "eslint": "^8.50.0",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "seedrandom": "^3.0.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "5.3.2",
    "typescript-collections": "^1.3.3",
    "vitest": "^0.34.6"
  }
}
