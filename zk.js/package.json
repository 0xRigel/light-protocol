{
  "name": "@lightprotocol/zk.js",
  "version": "0.3.2-alpha.16",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "start-test-validator": "./../cli/test_bin/run test-validator -b",
    "test": "pnpm test-zk.js && pnpm test-system-programs",
    "setup-env": "pnpm run start-test-validator && ts-mocha --max-old-space-size=4096 -t 2000000 tests/system-programs/public-functional.tests.ts --exit",
    "mint": "ts-mocha --max-old-space-size=4096 -t 2000000 tests/system-programs/mint-token.ts --exit",
    "build": "rimraf lib && pnpm tsc",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"tests/**/*.{ts,js}\" -w",
    "format:check": "prettier \"src/**/*{ts,js}\" --check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "format-staged": {
    "**/*.{js,jsx}": [
      "prettier --write"
    ]
  },
  "files": [
    "lib/**/*",
    "build-circuits/**/*",
    "bin/*",
    "circuit-lib/*"
  ],
  "keywords": [
    "sdk",
    "light",
    "lightprotocol"
  ],
