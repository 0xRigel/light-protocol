{
  "name": "@darjusch/light-protocol-sdk",
  "version": "1.0.27",
  "description": "light-sdk",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test-coverage": "npm test -- --coverage --collectCoverageFrom='./src/**'",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,js}\" && prettier --write \"tests/**/*.{ts,js}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "preversion": "npm run lint"
  },
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lightprotocol/light-sdk"
  },
  "keywords": [
    "sdk",
    "light",
    "lightprotocol"
  ],
  "author": "Darjusch Schrand",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lightprotocol/light-sdk/issues"
  },
  "homepage": "https://github.com/Lightprotocol/light-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "axios-mock-adapter": "^1.20.0",
    "jest": "^28.0.3",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@solana/wallet-adapter-base": "^0.9.5",
    "@solana/web3.js": "^1.41.4",
    "axios": "^0.27.2",
    "circomlib": "^0.5.5",
    "crypto": "^1.0.1",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.6.5",
    "ffjavascript": "^0.2.53",
    "n64": "^0.2.10",
    "snarkjs": "^0.4.16",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "bigint-buffer": "^1.1.5"
  }
}
