{
  "name": "@lightprotocol/zk-compression-cli",
  "version": "0.15.0",
  "description": "ZK Compression: Secure Scaling on Solana",
  "maintainers": [
    {
      "name": "Light Protocol maintainers",
      "email": "friends@lightprotocol.com"
    }
  ],
  "bin": {
    "light": "./test_bin/run"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "files": [
    "/accounts",
    "/bin",
    "/dist",
    "/test_bin",
    "./config.json",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@coral-xyz/anchor": "0.29.0",
    "@lightprotocol/compressed-token": "workspace:*",
    "@lightprotocol/hasher.rs": "workspace:*",
    "@lightprotocol/stateless.js": "workspace:*",
    "@oclif/core": "^4.0.23",
    "@oclif/plugin-autocomplete": "^3.2.4",
    "@oclif/plugin-help": "^6.2.12",
    "@oclif/plugin-not-found": "^3.2.21",
    "@oclif/plugin-plugins": "^5.4.9",
    "@solana-developers/helpers": "^2.5.5",
    "@solana/web3.js": "^1.95.3",
    "axios": "^1.7.7",
    "case-anything": "^2.1.13",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^2.9.2",
    "dotenv": "^16.4.5",
    "ffjavascript": "^0.3.0",
    "find-process": "^1.4.7",
    "node-fetch": "^3.3.2",
    "snake-case": "^3.0.4",
    "tar": "^6.2.1",
    "tweetnacl": "^1.0.3",
    "wait-on": "^7.2.0",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@lightprotocol/programs": "workspace:*",
    "@oclif/test": "4.0.9",
    "@solana/spl-token": "^0.4.8",
    "@types/bn.js": "^5.1.6",
    "@types/cli-progress": "^3.11.6",
    "@types/tar": "^6.1.13",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "5.2.1",
    "eslint-config-oclif-typescript": "^3.1.11",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-perfectionist": "^3.7.0",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vitest": "^1.6.0"
  },
  "oclif": {
    "bin": "light",
    "dirname": "light",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": ""
  },
  "scripts": {
    "add-bins": "./scripts/copyLocalProgramBinaries.sh && scripts/buildProver.sh",
    "postinstall": "[ -d ./bin ] && find ./bin -type f -exec chmod +x {} + || echo 'No bin directory found, skipping chmod'",
    "build": "shx rm -rf dist && pnpm tsc -p tsconfig.json && pnpm tsc -p tsconfig.test.json && pnpm add-bins",
    "format": "pnpm prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" -w",
    "format:check": "pnpm prettier \"src/**/*{ts,js}\"  \"test/**/*.{ts,js}\" --check",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "test-utils": "vitest run test/utils/index.test.ts",
    "test-config": "vitest run test/commands/config/index.test.ts",
    "test-create-mint": "vitest run test/commands/create-mint/index.test.ts",
    "test-merge-token-accounts": "vitest run test/commands/merge-token-accounts/index.test.ts",
    "test-create-token-pool": "vitest run test/commands/create-token-pool/index.test.ts",
    "test-approve-and-mint-to": "vitest run test/commands/approve-and-mint-to/index.test.ts",
    "test-mint-to": "vitest run test/commands/mint-to/index.test.ts",
    "test-transfer": "vitest run test/commands/transfer/index.test.ts",
    "test-balance": "vitest run test/commands/balance/index.test.ts",
    "test-compress-sol": "vitest run test/commands/compress-sol/index.test.ts",
    "test-decompress-sol": "vitest run test/commands/decompress-sol/index.test.ts",
    "test-compress-spl": "vitest run test/commands/compress-spl/index.test.ts",
    "test-decompress-spl": "vitest run test/commands/decompress-spl/index.test.ts",
    "test": "vitest run",
    "kill": "killall solana-test-validator || true && killall solana-test-val || true && sleep 1",
    "install-local": "pnpm build && pnpm global remove @lightprotocol/zk-compression-cli || true && pnpm global add $PWD",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "{workspaceRoot}/js",
          "{workspaceRoot}/programs",
          "{workspaceRoot}/gnark-prover"
        ],
        "outputs": [
          "{workspaceRoot}/bin",
          "{workspaceRoot}/dist",
          "{workspaceRoot}/lib",
          "{workspaceRoot}/test_bin"
        ]
      }
    }
  }
}
