{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@lightprotocol/tsconfig/base.json",
  "compilerOptions": {
    "outDir": "./lib",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "rootDirs": ["src", "generated"],
    "baseUrl": "src",
    "useUnknownInCatchVariables": false
  },
  "include": ["src"],
  "dry": false,
  "debug": false
}
